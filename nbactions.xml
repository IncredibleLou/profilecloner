<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath org.jboss.tfonteyne.profilecloner.Main --controller=orac.usersys.redhat.com --port=9999 --username=admin --password=12345678 /socket-binding-group=standard-sockets ss</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -classpath %classpath org.jboss.tfonteyne.profilecloner.Main --controller=orac.usersys.redhat.com --port=9999 --username=admin --password=12345678 /socket-binding-group=standard-sockets ss</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-classpath %classpath org.jboss.tfonteyne.profilecloner.Main --controller=orac.usersys.redhat.com --port=9999 --username=admin --password=12345678 /socket-binding-group=standard-sockets ss</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
    </actions>
